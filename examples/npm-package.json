  "cpu": ["x64", "arm64"],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.0.0"
  },
  "optionalDependencies": {
    "@aichat/cli-darwin-x64": "0.29.0",
    "@aichat/cli-darwin-arm64": "0.29.0",
    "@aichat/cli-linux-x64": "0.29.0",
    "@aichat/cli-linux-arm64": "0.29.0",
    "@aichat/cli-win32-x64": "0.29.0",
    "@aichat/cli-win32-arm64": "0.29.0"
