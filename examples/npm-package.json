{
  "name": "@aichat/cli",
  "version": "0.29.0",
  "description": "All-in-one LLM CLI tool - Node.js wrapper for AIChat",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aichat": "./examples/npm/aichat-wrapper.js"
  },
  "scripts": {
    "postinstall": "node examples/npm/download-binary.js",
    "test": "node examples/npm/test.js",
